#set("title") { Jesse Bounds }
#set("body") {
    <div id="main">
        <div class="posts">
        #for(post in posts) {
            <div class="post">
                <p><a href="/posts/#(post.link)">#(post.title)</a></p>
                <p>#(post.summary)</p>
                <p>#(post.formattedDate)</p>
            </div>
        }
        </div>
    </div>
}

#embed("master")
